{
  "version": "0.0.10",
  "name": "statebuild",
  "description": "React state management with OOP.",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "source": "src/index.ts",
  "files": ["build/*"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iliasbhal/statebuild.git"
  },
  "keywords": [
    "oop",
    "ddd",
    "react"
  ],
  "author": "Ilias Bhallil <ilias.bhal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iliasbhal/statebuild/issues"
  },
  "homepage": "https://github.com/iliasbhal/statebuild",
  "scripts": {
    "setup": "yarn build && yarn sync",
    "sync": "npm link && npm link statebuild",
    "dev": "yarn setup && yarn build:watch",
    "deploy": "yarn test && yarn build && npm publish",
    "test": "jest",
    "build": "yarn build:ts && yarn build:dts",
    "build:ts": "babel --extensions .ts,.tsx ./src -d ./build",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:watch": "tsc -w",
    "prepublish": "yarn build"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/types": "^28.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "babel-jest": "^29.2.2",
    "chokidar": "^3.5.3",
    "fs-extra": "^10.0.0",
    "global-jsdom": "^8.6.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.2.2",
    "jsdom": "^20.0.1",
    "jsdom-global": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.2.3"
  }
}
